CREATE TABLE `url_group_user` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `url_group_name` varchar(30) COLLATE utf8_unicode_ci NULL,
  PRIMARY KEY (`id`),
  FOREIGN KEY url_group_user(user_id) REFERENCES login_user(id) ON UPDATE CASCADE ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

