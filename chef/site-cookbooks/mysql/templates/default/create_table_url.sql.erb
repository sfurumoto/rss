CREATE TABLE `url` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `url` varchar(1000) COLLATE utf8_unicode_ci NOT NULL,
  `last_updating_time` DATETIME NULL,
  `website_bytes_number` SMALLINT UNSIGNED NULL,
  PRIMARY KEY (`id`),
  FOREIGN KEY url(user_id) REFERENCES login_user(id) ON UPDATE CASCADE ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

