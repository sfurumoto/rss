CREATE TABLE `url_group` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `group_id` int(11) NOT NULL,
  `url_id` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  FOREIGN KEY url_group_group_id(group_id) REFERENCES url_group_user(id) ON UPDATE CASCADE ON DELETE CASCADE,
  FOREIGN KEY url_group_url_id(url_id) REFERENCES url(id) ON UPDATE CASCADE ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

